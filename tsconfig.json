{
  "compilerOptions": {
    "module": "commonjs",
    "removeComments": false,
    "target": "es6",
    "esModuleInterop": true,
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "plugins": [{ "transform": "ts-nameof", "type": "raw" }],
    "typeRoots": ["node_modules/@types"],
    "lib": ["es2017", "dom"],
    "preserveConstEnums": true,
    "outDir": "../dist/server",
    "mapRoot": "../dist/server",
  }
}